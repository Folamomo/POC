clearvars;
close all;
clc;

lena = imread("lenaRGB.bmp");
figure();
imshow(lena);
%%
R = lena(:,:,1), 256);
G = histeq(lena(:,:,2), 256);
B = histeq(lena(:,:,3), 256);

%%
figure();
subplot(1, 3, 1);
imhist(R);
subplot(1, 3, 2);
imhist(G);
subplot(1, 3, 3);
imhist(B);

%%

R = histeq(lena(:,:,1), 256);
G = histeq(lena(:,:,2), 256);
B = histeq(lena(:,:,3), 256);
